<Window x:Class="ASK_projekt_zaliczeniowy.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ASK_projekt_zaliczeniowy"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="0,0,0,-16" RenderTransformOrigin="0.277,0.749">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="131*"/>
            <ColumnDefinition Width="17*"/>
            <ColumnDefinition Width="252*"/>
        </Grid.ColumnDefinitions>
        <StackPanel x:Name="Inputs" Margin="0,10,0,302" Grid.ColumnSpan="3">
            <Button x:Name="InputsButton"/>
        </StackPanel>
        <StackPanel x:Name="ValuesView" Margin="0,225,0,10" Grid.ColumnSpan="3">
            <TextBlock x:Name="AX" Height="50" Width="100" />
        </StackPanel>
    </Grid>
</Window>
